   "outputs": [],
   "source": [
    "\n",
    "x = range(0.0, 2*pi, 1000)\n",
    "y = sin.(3 .* x .+ 4 .* cos.(2 .* x));"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "75229f01-59ee-40a9-af6e-dc5b8e60c171",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "plot(x, y, color=\"blue\", linewidth=1.5, linestyle=\"-\")\n",
    "grid(true)\n",
    "title(\"A sinusoidally modulated sinusoid\");"
   ]
  },
